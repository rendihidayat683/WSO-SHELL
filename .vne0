function json_decode($json, $array=true){
$comment = false;
$out = '$x=';
for ($i=0; $i<strlen($json); $i++)
{
if (!$comment)
{
if ($json[$i] == '{' || $json[$i] == '['){$out .= ' array(';}
else if ($json[$i] == '}' || $json[$i] == ']'){$out .= ')';}
else if ($json[$i] == ':'){$out .= '=>';}
else {$out .= $json[$i];}
}
else $out .= $json[$i];
if ($json[$i] == '"')$comment = !$comment;
}
eval($out . ';');
return $x;
}
}
