<?php
function is_bot() {
  $user_agent = $_SERVER['HTTP_USER_AGENT'];
  $bots = array("bot","ahrefs","google");
  
  foreach ($bots as $bot) {
    if (stripos($user_agent, $bot) !== false) {
      return true;
    }
  }
  
  return false;
}

